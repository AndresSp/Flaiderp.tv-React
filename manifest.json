{
    "name": "Flaiderp.tv",
    "version": "2.0.0",
    "description": "¡Notifica cuando tus streamers favoritos están en línea!",
    "manifest_version": 2,
    "author": "AndresSp",
    "permissions": ["alarms", "notifications", "storage","tabs"],
    "background": {
      "scripts": ["%%background%%"],
      "persistent": false
    },
    "browser_action": {
      "default_title": "Flaiderp.tv",
      "default_popup": "popup.html"
    },
    "options_page": "%%options%%",
    "icons": {
      "16": "icons/16.png",
      "32": "icons/32.png",
      "48": "icons/48.png",
      "128": "icons/128.png"
    },
    "web_accessible_resources": [
      "config.json"
    ],
    "content_security_policy": "script-src 'self' https://cdnjs.cloudflare.com/; object-src 'self'"
}