{
    "name": "Flaiderp.tv",
    "version": "2.0.0",
    "description": "¡Notifica cuando tus streamers favoritos están en línea!",
    "manifest_version": 2,
    "author": "AndresSp",
    "permissions": ["alarms", "notifications", "storage","tabs"],
    "background": {
      "scripts": ["./background.js"],
      "persistent": false
    },
    "browser_action": {
      "default_title": "Flaiderp.tv",
      "default_popup": "./popup.html"
    },
    "options_page": "./options.js",
    "icons": {
      "16": "./assets/icon/16.png",
      "32": "./assets/icon/32.png",
      "48": "./assets/icon/48.png",
      "128": "./assets/icon/128.png"
    }
}