{
    "key": "",
    "name": "Flaiderp.tv",
    "version": "2.2.0",
    "description": "¡Notifica cuando tus streamers favoritos están en línea!",
    "manifest_version": 2,
    "author": "AndresSp",
    "permissions": ["alarms", "notifications", "storage", "identity", "https://*.twitch.tv/*"],
    "background": {
      "scripts": [
        "./background.js"]
    },
    "browser_action": {
      "default_title": "Flaiderp.tv",
      "default_popup": "./popup.html"
    },
    "icons": {
      "16": "assets/icon/16.png",
      "32": "assets/icon/32.png",
      "48": "assets/icon/48.png",
      "128": "assets/icon/128.png"
    },
    "browser_specific_settings": {
      "gecko": {
        "id": "{70c006f5-3c4e-4a1a-af04-e46194e01517}"
      }
    }
}