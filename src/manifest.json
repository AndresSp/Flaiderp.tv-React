{
    "name": "Flaiderp.tv",
    "version": "2.0.0",
    "description": "¡Notifica cuando tus streamers favoritos están en línea!",
    "manifest_version": 2,
    "author": "AndresSp",
    "permissions": ["alarms", "notifications", "storage", "identity", "http://localhost:3000/*", "https://*.twitch.tv/*"],
    "background": {
      "scripts": [
        "./background.js"]
    },
    "browser_action": {
      "default_title": "Flaiderp.tv",
      "default_popup": "./popup.html"
    },
    "icons": {
      "16": "./assets/icon/16.png",
      "32": "./assets/icon/32.png",
      "48": "./assets/icon/48.png",
      "128": "./assets/icon/128.png"
    },
    "browser_specific_settings": {
      "gecko": {
        "id": "{c4da9a78-998e-4659-b60c-31488f494315}"
      }
    }
}